#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>

int main() {
  int pid = fork();

  if (pid) {
    execl("/usr/bin/ls", "/usr/bin/ls", "-a", (char *)NULL);
  } else {
    execl("/usr/bin/ls", "/usr/bin/ls", "-a", "-l", "-h", (char *)NULL);
  }
  printf("DONE %d\n", pid);
}
